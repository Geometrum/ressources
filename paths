# Different paths

max_brightness_path="/sys/class/backlight/intel_backlight/max_brightness"
brightness_path="/sys/class/backlight/intel_backlight/brightness"
more_brightness="sudo ~/scripts/backlight.sh +"
less_brightness="sudo ~/scripts/backlight.sh -"

usb_mount_path="/run/media/${USER}/usb_"
dev_path="/dev"